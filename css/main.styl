.dashed-line
	height 20px
	left 134px
	position absolute
	top 50px
	width 130px
	border-bottom 5px dashed rgb( 255, 210, 0)

.row
	margin 0

.no-margin
	margin 0

.full-height
	height 100vh

footer
	background rgb( 255, 210, 0 )
	height 300px
	overflow auto

	#foot-info
		margin 4em auto 0
		min-width 1020px
		width 80%

		.network-solution
			.solution
				border 2px solid rgb( 255, 210, 0 )
				position absolute
				right 0
				width 185px

		.solutions
			display flex
			padding 0

			.solution
				display inline-block
				border-top 1px solid black
				font-size 1.1em
				height 200px
				margin 0 2em
				min-width 185px
				padding 0 1.5em
				p
					letter-spacing 1px

#adventages
	background-color rgb(243, 240, 231)
	margin 0
	padding 0.5em 0 0 0
	position relative

	h3
		font-weight 200
		text-align center
		text-transform uppercase
		margin 1em 0

		&::first-letter
			font-size 1.2em

	.btn
		align-items center
		background-color rgba(#E11A27, 0.8)
		border 1px solid rgba(#E11A27, 0.2)
		bottom -30px
		color rgba( 255, 255, 255, 0.9 )
		display flex
		flex-direction columns
		font-size 1.3em
		margin 0 auto
		padding 0.7em 1.7em
		position relative
		transition all 0.5s

		&:hover
			border 1px solid rgba(13,13,13,0.2)
			box-shadow 2px 2px 10px rgba(0,0,0,0.6)
			text-shadow 1px 1px 1px rgba(0,0,0,0.4)

	.content
		display flex
		margin 2em auto 1em
		overflow-x auto
		width 80%

		.adventage
			height 390px
			margin 0 auto
			min-width 300px
			width 300px

			&:hover
				border 1px solid rgba(243, 240, 231,0.2)
				box-shadow inset -3px -4px 15px rgba(0,0,0,0.2)
				text-shadow 1px 1px 1px rgba(0,0,0,0.1)

			img
				border-radius 50%
				display block
				margin 0 auto
				height 250px
				width 250px

			h4
				font-size 1.3em

			p
				margin 0
				padding 0.4em

#information
	height 90vh
	position relative
	top -10vh

	.solutions
		align-items center
		display flex
		height 100%
		justify-content center
		overflow-x auto
		padding 0.5em 0 0 1.2em
		z-index 0

		.margined
			margin 0 10%

		.solution
			height 300px
			min-height 300px
			min-width 310px
			width 310px

			.content
				background rgb(247, 240, 230)
				border-radius 0 0 15px 15px
				border-top 7px solid white
				font-size 1.1em
				height 237px
				padding 1.7em
				text-align left
			p
				span
					color red
					font-weight bold
					font-size 1.2em
			.title
				align-items center
				background rgb(255,210,0)
				border-radius 15px 15px 0 0
				display flex
				height 66px

				h4
					font-size 1.7em
					letter-spacing 5px
					margin 0 auto
					&::first-letter
						font-size 1.5em

	.btn-container
		bottom 10%
		display flex
		position absolute
		width 100%
		justify-content center
		.btn
			background-color rgba(#E11A27, 0.8)
			border 1px solid rgba(#E11A27, 0.2)
			color rgba( 255, 255, 255, 0.9 )
			font-size 1.3em
			padding 0.7em 1.7em
			transition all 0.5s

			&:hover
				border 1px solid rgba(13,13,13,0.2)
				box-shadow 2px 2px 10px rgba(0,0,0,0.6)
				text-shadow 1px 1px 1px rgba(0,0,0,0.4)

#page-options
	display flex
	justify-content center
	height 350px
	position relative

	.options
		align-items center
		bottom 15%
		display flex
		overflow-x auto

		.option
			height 250px
			margin 0 58px
			min-width 145px
			padding 0
			position relative
			transition all 0.4s
			width 145px
			border 1px solid rgba(255,210,0, 0)

			div
				align-items center
				background-color white
				border 5px solid rgb( 255, 210, 0)
				border-radius 50%
				display flex
				height 137px
				max-height 100%
				max-width 95%
				width 137px

				img
					display block
					height 48px
					margin 0 auto
					width 52px

			.description
				display block
				margin 0 auto
				font-size 1.5em
				max-width 134px
				padding 1em 0 0 0

#principal
	overflow hidden

	.home
		align-items center
		display flex
		flex-direction column
		height (100vh)
		padding-bottom -80px
		position relative

		h1, h3
			position relative
			top 15%

		h1
			letter-spacing 2px
			z-index 10

			&::first-letter
				font-size 1.3em

		h3
			font-size 1.3em
			font-weight 550
			z-index 10

		.bg-img
			background-position center
			background-size cover
			height 100%
			position absolute
			width 100%

		.btn
			background-color transparent
			border 1px solid rgba(33,33,33, 0.3)
			font-size 1.3em
			padding 0.7em 1.7em
			position relative
			top 25%
			transition all 0.5s

			&:hover
				border 1px solid rgba(13,13,13,0.2)
				box-shadow 2px 2px 10px rgba(0,0,0,0.6)
				text-shadow 1px 1px 1px rgba(0,0,0,0.4)

	#nav
		background white
		height 80px
		padding 25px 0 0 0
		position absolute
		width 100%
		z-index 10

		div
			height 30px

		.nav-contact
			padding 0

			a
				background-color: rgb( 255, 210, 0 )
				border-radius 10px
				color rgb( 33, 33, 33 )
				display inline-block
				margin -7px 0 0 0
				padding 0.5em 10%
				position relative
				white-space nowrap

				&:before
					border-top 2px dotted rgb( 33, 33, 33 )
					bottom 24%
					content ''
					left 10%
					margin 0 20px
					position absolute
					width 62%

				&:hover
					text-decoration none

		.nav-log
			.box-border
				border-bottom 2px solid black
				border-right 2px solid black
				border-top 2px solid black
				left 35%
				margin -6px 0 0 0
				position relative
				width 27px

				svg
					cursor pointer
					left -9px
					padding-top 12px
					position absolute

					line
						display inline-block
						stroke black
						stroke-width 1.5px

		.nav-menu
			padding 0.3em 0 0 0

			svg
				border-bottom 1.5px solid black
				border-top 1px solid black
				cursor pointer
				margin 0 0 0 25%
				padding-top 8px

				line
					stroke black
					stroke-width 1.5px

		.nav-title
			font-weight bold
			padding 0

			p
				color black
				font-size 20px
				white-space nowrap

				img
					height 19px
					position relative
					top -2px
					width 19px

#ready-solutions
	align-items flex-end
	justify-content center
	display flex
	position relative
	transition 0.6s

	.solutions
		align-items center
		bottom 0%
		display flex
		height 90%
		overflow-x auto
		position relative
		z-index 0

		.solution
			background rgb(255, 210, 0)
			border-radius 7px
			height 420px
			margin-left 100px
			min-width 314px
			text-align center
			width 314px

			&:first-child
				margin 0

			p
				font-size 1.3em
				margin 3% 0 0 0
				span
					font-size 0.8em

			.solution-title
				align-items center
				display flex
				height 118px
				position relative
				width 100%

				h4
					font-size 2em
					margin 0
					position absolute
					width 100%
					span
						font-size 1.2em

			.content
				background white
				font-size 1.3em
				height 255px
				margin 0 auto
				overflow auto
				position relative
				width 96%

				ul
					list-style none
					margin-left 9%
					margin-top 5%
					text-align left

					li
						position relative
						margin-bottom 0.4em

						&::before
							background-image url('/imgs/minis/list.png')
							background-size cover
							content ''
							display inline-block
							height 20px
							position absolute
							left -35px
							width 20px

				.price
					bottom 2%
					font-size 3em
					position absolute
					width 100%

	.title
		position absolute
		top 17%
		h3
			letter-spacing 2px
			margin 0
			text-align center
			text-transform uppercase
			top 0
			&::first-letter
				font-size 1.3em

#testimonials
	background #F7F0E6
	overflow hidden
	position relative

	.bg-img
		background-position center
		background-size cover
		height 100%
		position absolute
		transition all 2s
		width 100%

	.btn
		background-color transparent
		border 1px solid rgba(33,33,33, 0.3)
		bottom 5%
		font-size 1.3em
		left 17%
		padding 0.7em 1.7em
		position absolute
		transition all 0.5s

		&:hover
			border 1px solid rgba(13,13,13,0.2)
			box-shadow 2px 2px 10px rgba(0,0,0,0.6)
			text-shadow 1px 1px 1px rgba(0,0,0,0.4)

	.testimonial
		margin-left 10%
		overflow hidden
		position absolute

		h3
			font-size 3em
			letter-spacing 5px
			margin-bottom 1em
			margin-top 0.5em

			&::first-letter
				font-size 1.4em

		.owner-info
			position relative
			height 186px

			h4,h5
				left 35%
				position relative
				top 30%
				vertical-align center

			h4
				font-size 1.6em
				span
					font-size 1.3em

			h5
				font-size 1.3em

			.owner-pic
				background-size cover
				border-radius 50%
				height 166px
				position absolute
				width 166px

		.texto
			min-height 137px
			min-width 500px
			overflow auto
			width 500px

		.next-testimonial
			display inline-block
			position relative

			a
				color black
				padding-right 3em
				
				&:hover
					text-decoration none
				svg
					cursor pointer
					padding-top 12px
					position absolute
					right 0px

					line
						display inline-block
						stroke black
						stroke-width 1.5px


@media only screen and (max-width 700px)

	#information
		
		.solutions
			justify-content space-between
			overflow-x auto
			width 98%

			.margined
				margin 0 40px

			.solution
				height 250px
				min-height 250px
				min-width 260px
				width 310px

				.content
					border-top 4px solid white
					font-size 1em
					height 187px
					padding 1.6em
				p
					span
						font-size 1em

				.title
					height 46px

					h4
						font-size 1.5em
						letter-spacing 3px
						&::first-letter
							font-size 1.1em
		.btn-container
			justify-content center
			.btn
				transform scale(0.8)


	#adventages

		.btn
			transform scale(0.9)

		.content

			.adventage
				height 300px
				min-width 200px
				width 210px

				img
					height 160px
					width 160px
				h4
					font-size 1.2em

				p
					font-size 0.9em

	#page-options
		.options
			// bottom 0			
			.option
				height 205px

	#principal

		.home
			top 5%

			h1
				font-size 2em

			h3
				top 17%

			.btn
				top 25%

		#nav
			.nav-contact
				left -2%


	#ready-solutions
		padding-top 5em

		.solutions

			.solution
				border-radius 5px
				height 320px
				min-width 264px
				width 264px
				p
					span
						// font-size 0.8em

				.solution-title
					height 68px

					h4
						font-size 1.6em
						span
							font-size 1.1em

				.content
					height 205px

					ul
						// list-style none

						li
							margin-bottom 0.2em

					.price
						bottom 1%
						font-size 2em

	#testimonials
		// background #F7F0E6

		.bg-img
			// background-position center

		.btn
			bottom 1%
			left 12%
			transform scale(0.8)


		.testimonial
			// margin-left 10%

			h3
				font-size 2em

			.owner-info
				// position relative

				h4,h5
					left 2%
					top 65%

				h4
					font-size 1.2em
					span
						font-size 1.4em

				h5
					font-size 1em

				.owner-pic
					height 106px
					width 106px

			.texto
				max-height 110px
				min-width 300px
				width 300px

			.next-testimonial
				// display inline-block

				a
					// color black

					&:hover
						// text-decoration none
					svg
						// cursor pointer

						line
							// display inline-block


@media only screen and (max-width 400px)

	#adventages

		h3
			font-size 1.2em

		.btn
			transform scale(0.8)

		.content

			.adventage
				height 300px
				min-width 200px
				width 210px

				img
					height 160px
					width 160px

				h4
					font-size 1.2em

				p
					font-size 0.9em

	#principal
		.home

			h1
				font-size 2em
			
			.bg-img
				//

		#nav
			font-size 0.8em

			.nav-contact
				top 7px
				left -3px

			.nav-log
				left -2px


@media only screen and (max-height 500px) and (orientation landscape)

	#adventages

		h3
			font-size 1.2em
			margin-top -2px

		.btn
			transform scale(0.6)

		.content
			margin 0 5%
			margin-bottom -20px
			margin-top -5px
			width 90%
			.adventage
				height 220px
				min-width 150px
				width 160px

				img
					height 80px
					width 80px

				h4
					font-size 0.8em

				p
					font-size 0.7em